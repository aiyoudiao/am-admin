-- Model am_already comments


SET @preparedStatement = (SELECT IF(EXISTS
  (
    SELECT table_name 
            FROM INFORMATION_SCHEMA.TABLES
           WHERE table_schema = 'prisma'
             AND table_name LIKE 'am_already'
  ),
  CONCAT("ALTER TABLE am_already COMMENT ", "'This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments'"),
  "SELECT 1"
));
PREPARE alterIfExists FROM @preparedStatement;
EXECUTE alterIfExists;
DEALLOCATE PREPARE alterIfExists;



